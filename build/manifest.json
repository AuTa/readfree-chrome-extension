{
    "manifest_version": 2,

    "name": "ReadFree 传送门",
    "description": "ReadFree 电子书传送门",
    "version": "2.2",
    "icons": {
        "48": "favicon48.png",
        "128": "favicon128.png"
    },

    "content_scripts": [{
        "matches": ["http://book.douban.com/*", "http://www.douban.com/*", "http://www.amazon.cn/*"],
        "js": ["show.js"],
        "run_at": "document_end"
    }],
    "permissions": [
        "http://readfree.me/*"
    ]
}
